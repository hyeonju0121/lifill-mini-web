<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.mycompany.lifill_mini_web.dao.AddressDao"> 

	<insert id="aInsert" parameterType="Address">
		insert into address (mid, mzipcode, maddress)
		values(#{mid}, #{mzipcode}, #{maddress})
	</insert>
</mapper>